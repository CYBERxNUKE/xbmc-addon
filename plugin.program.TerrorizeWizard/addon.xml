<?xml version="1.1" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.TerrorizeWizard" name="Terrorize Wizard" version="1.3" provider-name="Terrorize Kodi">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Terrorize Wizard</summary>
    <description lang="en">Terrorize Wizard allows you to get access to the latest Terrorize Custom Builds.</description>
    <platform>all</platform>
  </extension>
</addon>