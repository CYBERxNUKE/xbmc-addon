<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.audio.radionl" name="Radio NL" version="1.0.6" provider-name="Beerkeeper">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.urlresolver"  version="0.0.1"/>
        <import addon="script.module.metahandler"  version="1.0.2"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>audio</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Alle radiozenders van Nederland</summary>
        <description lang="en">Tot dusver bekend alle radiozenders die in Nederland worden uitgezonden</description>
        <disclaimer lang="en"></disclaimer>
        <language></language>
        <platform>all</platform>
        <license></license>
        <forum></forum>
        <website>http://www.google.nl</website>
        <email></email>
        <source></source>
    </extension>
</addon>

<addon id="repository.beerkeeper" name="Beerkeeper's Repository" version="1.0" provider-name="Beerkeeper">
<extension point="xbmc.addon.repository" name="Beerkeeper Addon Repository">
<info compressed="true">http://xbmc-addon.googlecode.com/svn/Other/addons.xml</info>
<checksum>http://xbmc-addon.googlecode.com/svn/Other/addons.xml.md5</checksum>
<datadir zip="true">http://xbmc-addon.googlecode.com/svn/Other/</datadir>
</extension>
<extension point="xbmc.addon.metadata">
<summary>Beerkeeper's Repository</summary>
<description>Beerkeeper's Repository</description>
<platform>all</platform>
</extension>
</addon>
</addons>
