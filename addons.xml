<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.nukewizard" version="1.0.0" name="Computertechs Installer" provider-name="computertechs.org">
	<requires>
		<import addon="xbmc.python" version="2.1.0"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>executable</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>The Computertechs Installer</summary>
		<description>Configure your XBMC</description>
		<website>http://www.computertechs.org/</website>
		<platform>all</platform>
	</extension>
	<extension point="xbmc.addon.repository" name="CYBERxNUKE Addon Repository">
		 <info compressed="true">https://github.com/CYBERxNUKE/xbmc-addon/blob/master/addons.xml</info>
		 <checksum>https://github.com/CYBERxNUKE/xbmc-addon/blob/master/addons.xml.md5</checksum>
		 <datadir zip="true">https://github.com/CYBERxNUKE/xbmc-addon/blob/master/</datadir>
	</extension>
</addon>

<addon id="plugin.video.stalker" name="IPTV Stalker PLUS" version="10.2.3" provider-name="dara, couch potato">
    <requires>
		<import addon="xbmc.python" version="2.14.0"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="addon.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.service" library="server.py" start="startup" />
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="en">IPTV Stalker PLUS</summary>
		<description lang="en">IPTV Stalker + VOD add-on for PAPIAO servers, based on original code by esxbr</description>
		<license>GNU General Public License, v2</license>
		<language></language>
		<forum>http://iptvtalk.org/</forum>
	</extension>
</addon>

<addon id="repository.cyberxnuke" name="CYBERxNUKE Repository" version="1.1" provider-name="CYBERxNUKE">
<extension point="xbmc.addon.repository" name="CYBERxNUKE Addon Repository">
<info compressed="true">https://github.com/CYBERxNUKE/xbmc-addon/raw/master/addons.xml</info>
<checksum>https://github.com/CYBERxNUKE/xbmc-addon/raw/master/addons.xml</checksum>
<datadir zip="true">https://github.com/CYBERxNUKE/xbmc-addon/raw/master/</datadir>
</extension>
<extension point="xbmc.addon.metadata">
<summary>CYBERxNUKE Repository</summary>
<description>CYBERxNUKE Repository</description>
<platform>all</platform>
</extension>
</addon>
</addons>
