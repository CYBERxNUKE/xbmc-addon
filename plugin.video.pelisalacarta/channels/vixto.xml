<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<channel>
	<id>vixto</id>
	<name>Vixto</name>

	<active>true</active>
	<adult>false</adult>
	<language>es</language>

	<version>2</version>
	<changes>
		<change>
			<date>16/02/2017</date>
			<autor>Vixto</autor>
			<description>Correccion para el apartado de series</description>
		</change>
		<change>
			<date>12/11/2016</date>
			<autor>Cmos</autor>
			<description>Primera version, sustituye a oranline</description>
		</change>
	</changes>

	<fanart></fanart>
	<thumbnail>http://i.imgur.com/y4c4HT2.png</thumbnail>
	<bannermenu>http://media.tvalacarta.info/pelisalacarta/bannermenu/vixto.png</bannermenu>
	<update_url>https://raw.githubusercontent.com/tvalacarta/pelisalacarta/master/python/main-classic/channels/</update_url>

	<categories>
		<category>latino</category>
		<category>movie</category>
		<category>serie</category>
		<category>vos</category>
	</categories>

	<settings>
		<include_in_global_search>disabled</include_in_global_search>
	</settings>
	<settings>
		<id>include_in_newest_peliculas</id>
		<type>bool</type>
		<label>Incluir en Novedades - Películas</label>
		<default>true</default>
		<enabled>true</enabled>
		<visible>true</visible>
	</settings>
	<settings>
		<id>modo_grafico</id>
		<type>bool</type>
		<label>Buscar información extra</label>
		<default>true</default>
		<enabled>true</enabled>
		<visible>true</visible>
	</settings>
	<settings>
		<id>perfil</id>
		<type>list</type>
		<label>Perfil de color</label>
		<default>0</default>
		<enabled>true</enabled>
		<visible>true</visible>
		<lvalues>Perfil 3</lvalues>
		<lvalues>Perfil 2</lvalues>
		<lvalues>Perfil 1</lvalues>
	</settings>
	<settings>
		<id>filterlanguages</id>
		<type>list</type>
		<label>Mostrar enlaces en idioma...</label>
		<default>3</default>
		<enabled>true</enabled>
		<visible>true</visible>
		<lvalues>VOSE</lvalues>
		<lvalues>Latino</lvalues>
		<lvalues>Español</lvalues>
		<lvalues>No filtrar</lvalues>
	</settings>
	<settings>
		<id>filterlinks</id>
		<type>list</type>
		<label>Mostrar enlaces de tipo...</label>
		<default>2</default>
		<enabled>true</enabled>
		<visible>true</visible>
		<lvalues>Solo Descarga</lvalues>
		<lvalues>Solo Online</lvalues>
		<lvalues>No filtrar</lvalues>
	</settings>
	<settings>
		<id>orderlinks</id>
		<type>list</type>
		<label>Ordenar enlaces por...</label>
		<default>2</default>
		<enabled>true</enabled>
		<visible>true</visible>
		<lvalues>Servidor</lvalues>
		<lvalues>Idioma</lvalues>
		<lvalues>Más recientes</lvalues>
	</settings>
</channel>
