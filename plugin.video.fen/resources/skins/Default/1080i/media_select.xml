<?xml version="1.0" encoding="UTF-8"?>
<window>
    <controls>
        <control type="image">
            <left>0</left>
            <top>0</top>
            <width>1920</width>
            <height>1080</height>
            <texture>common/black.png</texture>
            <animation type="WindowOpen" reversible="false">
                <effect type="fade" start="0" end="80" time="200" tween="sine" easing="in" />
            </animation>
        </control>
        <control type="group">
            <left>100</left>
            <top>135</top>
            <animation type="WindowOpen" reversible="false">
                <effect type="zoom" start="50" end="100" time="200" center="770,560" tween="quadratic" easing="out" />
            </animation>
            <control type="image">
                <height>810</height>
                <width>1720</width>
                <texture colordiffuse="FF302F2F">common/white.png</texture>
            </control>
            <control type="image">
                <left>7</left>
                <top>7</top>
                <width>45</width>
                <height>45</height>
                <align>right</align>
                <aligny>top</aligny>
                <aspectratio aligny="top">keep</aspectratio>
                <texture colordiffuse="B3CCCCCC">common/fen.png</texture>
            </control>
            <control type="image">
                <left>1170</left>
                <top>10</top>
                <height max="792">auto</height>
                <width max="530">auto</width>
                <align>center</align>
                <aligny>center</aligny>
                <aspectratio>keep</aspectratio>
                <texture>$INFO[Container(2025).ListItem.Property(icon)]</texture>
            </control>
            <control type="fixedlist" id="2025">
                <pagecontrol>2065</pagecontrol>
                <left>0</left>
                <top>70</top>
                <width>1140</width>
                <height>720</height>
                <onup>2025</onup>
                <ondown>2025</ondown>
                <onright>2065</onright>
                <scrolltime tween="sine">200</scrolltime>
                <orientation>Vertical</orientation>
                <focusposition>3</focusposition>
                <movement>4</movement>
                <itemlayout height="90" width="1140">
                    <control type="group">
                        <description>Only Line1</description>
                        <visible>String.IsEqual(Window().Property(multi_line),false)</visible>
                        <width max="1070">auto</width>
                        <left>40</left>
                        <control type="label">
                            <top>45</top>
                            <height>5</height>
                            <font>font12</font>
                            <textcolor>FFCCCCCC</textcolor>
                            <aligny>center</aligny>
                            <label>[UPPERCASE]$INFO[ListItem.Property(line1)][/UPPERCASE]</label>
                        </control>
                    </control>
                    <control type="group">
                        <description>Line1 and Line2</description>
                        <visible>String.IsEqual(Window().Property(multi_line),true)</visible>
                        <width max="1070">auto</width>
                        <left>40</left>
                        <control type="label">
                            <top>25</top>
                            <height>5</height>
                            <font>font12</font>
                            <textcolor>FFCCCCCC</textcolor>
                            <aligny>center</aligny>
                            <label>[UPPERCASE]$INFO[ListItem.Property(line1)][/UPPERCASE]</label>
                        </control>
                        <control type="label">
                            <top>60</top>
                            <height>5</height>
                            <font>font12</font>
                            <textcolor>FFA9A8A8</textcolor>
                            <aligny>center</aligny>
                            <label>[UPPERCASE][I]$INFO[ListItem.Property(line2)][/I][/UPPERCASE]</label>
                        </control>
                    </control>
                </itemlayout>
                <focusedlayout height="90" width="1140">
                    <control type="image">
                        <width>1200</width>
                        <height>90</height>
                        <aligny>top</aligny>
                        <texture colordiffuse="FFCCCCCC">common/white.png</texture>
                        <animation effect="fade" start="100" end="50" time="0" condition="!Control.HasFocus(2025)">Conditional</animation>
                    </control>
                    <control type="image">
                        <width>30</width>
                        <height>90</height>
                        <aligny>top</aligny>
                        <texture colordiffuse="$INFO[Window(10000).Property(fen.highlight)]">common/white.png</texture>
                        <animation effect="fade" start="100" end="50" time="0" condition="!Control.HasFocus(2025)">Conditional</animation>
                    </control>
                    <control type="group">
                        <description>Line1</description>
                        <visible>String.IsEqual(Window().Property(multi_line),false)</visible>
                        <width max="1070">auto</width>
                        <left>40</left>
                        <control type="label">
                            <top>45</top>
                            <height>5</height>
                            <font>font12</font>
                            <textcolor>FF4a4347</textcolor>
                            <aligny>center</aligny>
                            <label>[UPPERCASE]$INFO[ListItem.Property(line1)][/UPPERCASE]</label>
                        </control>
                    </control>
                    <control type="group">
                        <description>Line1 and Line2</description>
                        <visible>String.IsEqual(Window().Property(multi_line),true)</visible>
                        <width max="1070">auto</width>
                        <left>40</left>
                        <control type="label">
                            <top>25</top>
                            <height>5</height>
                            <font>font12</font>
                            <textcolor>FF4a4347</textcolor>
                            <aligny>center</aligny>
                            <label>[UPPERCASE]$INFO[ListItem.Property(line1)][/UPPERCASE]</label>
                        </control>
                        <control type="label">
                            <top>60</top>
                            <height>5</height>
                            <font>font12</font>
                            <textcolor>FF656565</textcolor>
                            <aligny>center</aligny>
                            <label>[UPPERCASE][I]$INFO[ListItem.Property(line2)][/I][/UPPERCASE]</label>
                        </control>
                    </control>
                </focusedlayout>
            </control>
            <control type="scrollbar" id="2065">
                <left>1155</left>
                <top>70</top>
                <width>15</width>
                <height>720</height>
                <texturesliderbackground colordiffuse="FF302F2F">common/white.png</texturesliderbackground>
                <texturesliderbar colordiffuse="FF555556">common/white.png</texturesliderbar>
                <texturesliderbarfocus colordiffuse="$INFO[Window(10000).Property(fen.highlight)]">common/white.png</texturesliderbarfocus>
                <onleft>2025</onleft>
                <onright>2025</onright>
                <showonepage>false</showonepage>
                <orientation>vertical</orientation>
            </control>
        </control>
    </controls>
</window>