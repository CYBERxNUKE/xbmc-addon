<?xml version="1.0" encoding="UTF-8"?>
<window>
    <coordinates>
        <left>0</left>
        <top>0</top>
    </coordinates>
    <controls>
        <control type="group" id="5">
            <control type="image">
                <description>Background Image</description>
                <posx>0</posx>
                <posy>0</posy>
                <width>1920</width>
                <height>1080</height>
                <texture border="2">bg_full.png</texture>
            </control>
            <control type="image">
                <description>Header image</description>
                <posx>0</posx>
                <posy>0</posy>
                <width>1920</width>
                <height>95</height>
                <texture border="2">header_full.png</texture>
            </control>
            <control type="image">
                <description>Footer image</description>
                <posx>0</posx>
                <posy>985</posy>
                <width>1920</width>
                <height>95</height>
                <texture border="2">header_full.png</texture>
            </control>
            <control type="image">
                <description>Bug Report image</description>
                <posx>65</posx>
                <posy>120</posy>
                <width>300</width>
                <height>300</height>
                <texture border="2">bugs.png</texture>
            </control>
            <control type="label" id="101">
                <description>header label</description>
                <posx>0</posx>
                <posy>20</posy>
                <width>1920</width>
                <height>60</height>
                <font>font13</font>
                <label>Bug Reporting</label>
                <textcolor>$INFO[Window().Property(dhtext)]</textcolor>
                <align>center</align>
                <aligny>center</aligny>
            </control>
            <control type="textbox" id="102">
                <description>Main Text</description>
                <left>440</left>
                <top>125</top>
                <width>1400</width>
                <height>300</height>
                <align>left</align>
                <aligny>top</aligny>
                <label></label>
            </control>
            <control type="edit" id="103">
                <description>Input Text</description>
                <left>95</left>
                <top>895</top>
                <width>1700</width>
                <height>70</height>
                <visible>true</visible>
                <aligny>center</aligny>
                <textoffsetx>25</textoffsetx>
                <hinttext>Type your bug report details in this box</hinttext>
                <textcolor>FF916f91</textcolor>
                <texturefocus>Buttons/plainbtn_focus.png</texturefocus>
                <texturenofocus>Buttons/plainbtn_focus.png</texturenofocus>
                <onup>105</onup>
                <ondown>15</ondown>
                <onleft>201</onleft>
                <onright>105</onright>
            </control>
            <control type="textbox">
                <description>Open Bugs Text</description>
                <left>0</left>
                <top>450</top>
                <width>1920</width>
                <height>70</height>
                <align>center</align>
                <aligny>center</aligny>
                <label>Open Bugs Reported by Users</label>
                <textcolor>$INFO[Window().Property(dhtext)]</textcolor>
            </control>
            <control type="textbox" id="104">
                <description>Report Contents</description>
                <left>85</left>
                <top>520</top>
                <width>1670</width>
                <height>350</height>
                <font>font10</font>
                <align>left</align>
                <aligny>top</aligny>
                <pagecontrol>105</pagecontrol>
                <label></label>
            </control>
            <control type="scrollbar" id="105">
                <description>Scrollbar</description>
                <left>1870</left>
                <top>520</top>
                <width>30</width>
                <height>350</height>
                <subtype>text</subtype>
                <visible>true</visible>
                <texturesliderbackground>Buttons/scrollbar-v-background.png</texturesliderbackground>
                <texturesliderbar>Buttons/scrollbar-v.png</texturesliderbar>
                <texturesliderbarfocus>Buttons/scrollbar-v-focus.png</texturesliderbarfocus>
                <orientation>vertical</orientation>
                <showonepage>true</showonepage>
                <onup>104</onup>
                <ondown>104</ondown>
                <onleft>103</onleft>
                <onright>103</onright>
            </control>
            <control type="grouplist" id="15">
                <description>Button Group</description>
                <left>0</left>
                <top>1009</top>
                <width>1920</width>
                <height>106</height>
				<orientation>horizontal</orientation>
				<align>center</align>
				<itemgap>14</itemgap>
                <defaultcontrol>201</defaultcontrol>
                <visible>true</visible>
                <onup>103</onup>
                <ondown>103</ondown>
                <onleft>103</onleft>
                <onright>103</onright>
                <control type="button" id="201">
                    <description>Submit Button</description>
                    <width>240</width>
                    <height>50</height>
                    <font>font12</font>
                    <textcolor>FFF0F0F0</textcolor>
                    <textoffsetx>20</textoffsetx>
                    <label>Submit</label>
                    <align>center</align>
                    <aligny>center</aligny>
                    <texturefocus colordiffuse="$VAR[HighlightBarColor]">Buttons/plainbtn_focus.png</texturefocus>
                    <texturenofocus>Buttons/pixel.png</texturenofocus>
                    <onup>103</onup>
                    <ondown>103</ondown>
                    <onleft>202</onleft>
                    <onright>202</onright>
                </control>
                <control type="button" id="202">
                    <description>Close Button</description>
                    <width>240</width>
                    <height>50</height>
                    <font>font12</font>
                    <textcolor>FFF0F0F0</textcolor>
                    <textoffsetx>20</textoffsetx>
                    <label>Cancel</label>
                    <align>center</align>
                    <aligny>center</aligny>
                    <texturefocus colordiffuse="$VAR[HighlightBarColor]">Buttons/plainbtn_focus.png</texturefocus>
                    <texturenofocus>Buttons/pixel.png</texturenofocus>
                    <onup>103</onup>
                    <ondown>103</ondown>
                    <onleft>201</onleft>
                    <onright>203</onright>
                </control>
            </control>
        </control>
    </controls>
</window>